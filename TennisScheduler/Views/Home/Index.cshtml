@model List<List<DateTime>>
<link href="~/Content/SiteModule.css" rel="Stylesheet" type="text/css" />
<script src="~/Scripts/jquery-ui-1.8.11.min.js" type="text/javascript"></script>
<script src="~/Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
<script src="~/Scripts/Calender/Index.js"></script>
<div id="component-header">
    <a 
       class="month" style="float:left;margin-left:10px;">Prev</a>
    @DateTime.Now.ToString("MMM") @DateTime.Now.Year <a 
                                                        class="month" style="float:right;margin-right:10px;">Next</a>
</div>

<div>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>
                    Sun
                </th>
                <th>
                    Mon
                </th>
                <th>
                    Tue
                </th>
                <th>
                    Wed
                </th>
                <th>
                    Thurs
                </th>
                <th>
                    Fri
                </th>
                <th>
                    Sat
                </th>
            </tr>
        </thead>
        <tbody>
           @foreach (var i in Model)
           {
               <tr>
               @foreach (var item in i)
               {
                   if (item.Ticks == 0)
                   {
                       <td></td>
                   }
                   else
                   {
                    <td>@item.Day</td>
                   }

               }

                </tr>
           } 
        </tbody>
    </table>
</div>